  <rdf:Description rdf:about="http://purl.org/net/vocab/2003/11/photo#maxFocalLength"><ns0:comment xmlns:ns0="http://www.w3.org/2000/01/rdf-schema#" xml:lang="en">The maximum focal length of a lens range, expressed in milimeters.</ns0:comment><ns0:isDefinedBy xmlns:ns0="http://www.w3.org/2000/01/rdf-schema#" rdf:resource="http://purl.org/net/vocab/2003/11/photo"/><ns0:domain xmlns:ns0="http://www.w3.org/2000/01/rdf-schema#" rdf:resource="http://purl.org/net/vocab/2003/11/photo#LensRange"/><rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
