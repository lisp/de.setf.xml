  <rdf:Description rdf:about="http://purl.org/net/vocab/2003/11/photo#serial_sha1sum"><ns0:subPropertyOf xmlns:ns0="http://www.w3.org/2000/01/rdf-schema#" rdf:resource="http://purl.org/net/vocab/2004/10/gadget#serial_sha1sum"/><ns0:isDefinedBy xmlns:ns0="http://www.w3.org/2000/01/rdf-schema#" rdf:resource="http://purl.org/net/vocab/2003/11/photo"/><rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/><rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/><ns0:label xmlns:ns0="http://www.w3.org/2000/01/rdf-schema#" xml:lang="en">sha1 of serial number</ns0:label>
    <ns0:comment xmlns:ns0="http://www.w3.org/2000/01/rdf-schema#" xml:lang="en">The SHA1sum of serial number for a piece of photography equipment, expressed as 40 lowercase hexadecimal digits.</ns0:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/vocab/2003/11/photo#serial"><ns0:isDefinedBy xmlns:ns0="http://www.w3.org/2000/01/rdf-schema#" rdf:resource="http://purl.org/net/vocab/2003/11/photo"/><rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/><rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/><ns0:label xmlns:ns0="http://www.w3.org/2000/01/rdf-schema#" xml:lang="en">serial number</ns0:label><ns0:subPropertyOf xmlns:ns0="http://www.w3.org/2000/01/rdf-schema#" rdf:resource="http://purl.org/net/vocab/2004/10/gadget#serial"/>
