  <rdf:Description rdf:about="http://purl.org/net/vocab/2003/11/photo#similarity_hash"><ns0:example xmlns:ns0="http://purl.org/vocab/vann/" xml:lang="en">
&lt;foaf:Image rdf:about="http://www.wasab.dk/morten/2005/04/photos/2/t/image-2.jpg"&gt;
  &lt;photo:similarity_hash&gt;5555455555f60f60f60f50f30f40f60f40f60&lt;/photo:similarity_hash&gt;
&lt;/foaf:Image&gt;</ns0:example><ns0:range xmlns:ns0="http://www.w3.org/2000/01/rdf-schema#" rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/><ns0:label xmlns:ns0="http://www.w3.org/2000/01/rdf-schema#" xml:lang="en">similarity hash</ns0:label><ns0:isDefinedBy xmlns:ns0="http://www.w3.org/2000/01/rdf-schema#" rdf:resource="http://purl.org/net/vocab/2003/11/photo"/><rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/><ns0:domain xmlns:ns0="http://www.w3.org/2000/01/rdf-schema#" rdf:resource="http://xmlns.com/foaf/0.1/Image"/>
